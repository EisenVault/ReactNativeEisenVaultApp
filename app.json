{
  "expo": {
    "name": "ReactNativeEisenVaultApp",
    "slug": "ReactNativeEisenVaultApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/android/mipmap-hdpi/ic_launcher_foreground.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ReactNativeEisenVaultApp",
      "icon": "./assets/ios/AppIcon.appiconset/iTunesArtwork@2x.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/mipmap-hdpi/ic_launcher_foreground.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.ReactNativeEisenVaultApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
